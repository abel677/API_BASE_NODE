generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String   @unique
  email     String   @unique
  password  String
  createdAt DateTime
  updatedAt DateTime
  roles     Rol[]
}

model Rol {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime
  updatedAt DateTime
  users     User[]
}
